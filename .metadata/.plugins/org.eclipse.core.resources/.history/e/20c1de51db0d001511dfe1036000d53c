package week2examples.day1;

public class ConvertStringToPrimitiveType
{

	public static void main(String[] args)
	{
		String strAge = "10";
		System.out.println(strAge);
		strAge = strAge + 23;
		System.out.println(strAge);
		
		int intAge = Integer.parseInt(strAge);
		
		intAge += 23;
		System.out.println(strAge);
		
		
		
		String strSalary = "3456.78";
		
		

	}

}
